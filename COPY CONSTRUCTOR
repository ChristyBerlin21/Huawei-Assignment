#include<iostream>
using namespace std; 
class values
{
private:
    int x, y;
public:
    values(int x1, int y1) {
        x = x1; 
        y = y1; }
    // Copy constructor
    values(const values &p1) {
        x = p1.x;
        y = p1.y;
    }
    int get X(
   {  
        return x;
 }
  int get Y()            
 {  
       return y;
 }
};
int main()
{
    values p1(10, 15); // Normal constructor is called here
    values p2 = p1; // Copy constructor is called here
    // Let us access values assigned by constructors
    return 0;
}
